                                <table class="table table-hover table-striped table-bordered table-dark">
                                        <caption>List of Job Applicants</caption>
                                        <thead>
                                          <tr>
                                            <th scope="col">#S.NO</th>
                                            <th scope="col">USERNAME</th>
                                            <th scope="col">EMAIL</th>
                                            <th scope="col">CONTACT</th>
                                            <th scope="col">JOB-TITLE</th>
                                            <th scope="col">COMPANY</th>
                                            <th scope="col">JOB-CITY</th>
                                            <th scope="col">SALARY PA</th>
                                            <th scope="col">APPLIED-DATE</th>
                                            <th scope="col">DELETE</th>
                                          </tr>
                                        </thead>
                                        <tbody>
                                    <% for(jobApp of jobApps){ %>
                                        <% x++ %>
                                                      <tr>
                                                        <th scope="row"><%=x%></th>
                                                        <td class="text-capitalize"><%=jobApp.username%></td>
                                                        <td><%=jobApp.email%></td>
                                                        <td class="text-capitalize"><%=jobApp.contact%></td>
                                                        <td class="text-capitalize"><%=jobApp.jobTitle%></td>
                                                        <td class="text-capitalize"><%=jobApp.jobCompany%></td>
                                                        <td class="text-capitalize"><%=jobApp.city%></td>
                                                        <td class="text-capitalize"><%=jobApp.salary%></td>
                                                        <td class="text-capitalize"><%=jobApp.jobAppliedDate%></td>
                                                        <td>
                                                            <form action="/admin/delete-job-application" method="POST" class="m-0 p-0">
                                                            <input type="hidden" name="_csrf" value="<%=csrfToken%>">
                                                            <input type="hidden" name="jobAppId" value="<%=jobApp._id%>">
                                                            <button class="btn btn-danger bg-gradient-danger btn-sm">Delete</button>
                                                            </form>
                                                        </td>
                                                      </tr>
                                        <% } %>
                                    </tbody>
                                </table>
                          </div>
                            <% } else { %>
                              <h3 class="text-default"> No Job Applicants Found! :(</h3>
                              <% } %>
                              <% } else if(path === '/admin/cert-data') { %>
                                <h2 class="text-center mt-3 mb-5">Registered Certification Data </h2>
                                <% if(certDatas.length>0) { %>
                                    <% var x = 0; %>
                                  <div class="table-responsive">
                                        <table class="table table-hover table-striped table-bordered table-dark">
                                                <caption>List of Cert Data</caption>
                                                <thead>
                                                  <tr>
                                                    <th scope="col">#S.NO</th>
                                                    <th scope="col">Name</th>
                                                    <th scope="col">Email</th>
                                                    <th scope="col">Phone</th>
                                                    <th scope="col">College</th>
                                                    <th scope="col">City</th>
                                                    <th scope="col">Parent-Ph</th>
                                                    <th scope="col">Cert-Course</th>
                                                    <th scope="col">Applied-date</th>
                                                    <th scope="col">DELETE</th>
                                                  </tr>
                                                </thead>
                                                <tbody>
                                            <% for(certData of certDatas){ %>
                                                <% x++ %>
                                                              <tr>
                                                                <th scope="row"><%=x%></th>
                                                                <td class="text-capitalize"><%=certData.name%></td>
                                                                <td><%=certData.email%></td>
                                                                <td class="text-capitalize"><%=certData.phone%></td>
                                                                <td class="text-capitalize"><%=certData.college%></td>
                                                                <td class="text-capitalize"><%=certData.city%></td>
                                                                <td class="text-capitalize"><%=certData.parentphone%></td>
                                                                <td class="text-capitalize"><%=certData.certification%></td>
                                                                <td class="text-capitalize"><%=certData.createdAt%></td>
                                                                <td>
                                                                    <form action="/admin/delete-cert-data" method="POST" class="m-0 p-0">
                                                                    <input type="hidden" name="_csrf" value="<%=csrfToken%>">
                                                                    <input type="hidden" name="certDataId" value="<%=certData._id%>">
                                                                    <button class="btn btn-danger bg-gradient-danger btn-sm">Delete</button>
                                                                    </form>
                                                                </td>
                                                              </tr>
                                                <% } %>
                                            </tbody>
                                        </table>
                                  </div>
                                    <% } else { %>
                                      <h3 class="text-default"> Certification Data Not Found! :(</h3>
                                      <% } %>
                              <% } else if(path === '/admin/manage-bugapplications') { %>
                                <h2 class="text-center mt-3 mb-5">Reported Bugs In The Website </h2>
                                <% if(bugApps.length>0) { %>
                                    <% var x = 0; %>
                                  <div class="table-responsive">
                                        <table class="table table-hover table-striped table-bordered table-dark">
                                                <caption>List of Reported Bugs</caption>
                                                <thead>
                                                  <tr>
                                                    <th scope="col">#S.NO</th>
                                                    <th scope="col">USERNAME</th>
                                                    <th scope="col">EMAIL</th>
                                                    <th scope="col">Bug Desc.</th> 
                                                  </tr>
                                                </thead>
                                                <tbody>
                                            <% for(bugApp of bugApps){ %>
                                                <% x++ %>
                                                              <tr>
                                                                <th scope="row"><%=x%></th>
                                                                <td class="text-capitalize"><%=bugApp.name%></td>
                                                                <td><%=bugApp.email%></td>
                                                                <td class="text-capitalize small"><%=bugApp.bugmessage%></td>
                                                                <td>
                                                                    <form action="/admin/delete-bug-application" method="POST" class="m-0 p-0">
                                                                    <input type="hidden" name="_csrf" value="<%=csrfToken%>">
                                                                    <input type="hidden" name="bugAppId" value="<%=bugApp._id%>">
                                                                    <button class="btn btn-danger bg-gradient-danger btn-sm">Delete</button>
                                                                    </form>
                                                                </td>
                                                              </tr>
                                                <% } %>
                                            </tbody>
                                        </table>
                                  </div>
                                    <% } else { %>
                                      <h3 class="text-default"> No Bugs Found! :(</h3>
                                      <% } %>
                                   
        

                        <% } else { %>
                        <h3 class="text-center">You've Entered into Wrong Content this will not work</h3>
                        <% } %>
                 </div>  
            </div>
    </section>
  </div>
</main>

