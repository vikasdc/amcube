<%- include('../includes/header.ejs') %>
<link href="https://unpkg.com/gijgo@1.9.13/css/gijgo.min.css" rel="stylesheet" type="text/css" />
<body>
  <%- include('../includes/adminnav.ejs') %>
<main>
<div class="position-relative">
<section class="section-shaped my-0" style="min-height:80vh">
        <div class="row mt-3 mb-5">
        <div class="col mx-3 p shadow-sm mb-5">          
            <% if(path === '/admin/manage-testapplications') {  %>
                <h2 class="text-center mt-3 mb-5">Registered Test Applicants</h2>
                <% if(testApps.length>0) { %>
                    <% var x = 0; %>
                  <div class="table-responsive ">
                        <table class="table table-hover table-striped table-bordered table-dark">
                                <caption>List of Test Applicants</caption>
                                <thead>
                                  <tr>
                                    <th scope="col">#S.NO</th>
                                    <th scope="col">USERNAME</th>
                                    <th scope="col">EMAIL</th>
                                    <th scope="col">CONTACT</th>
                                    <th scope="col">GENDER</th>
                                    <th scope="col">DOB</th>
                                    <th scope="col">DEGREE</th>
                                    <th scope="col">ADDRESS</th>
                                    <th scope="col">TEST-CITY</th>
                                    <th scope="col">TEST-DATE</th>
                                    <th scope="col">TEST-TIME</th>
                                    <th scope="col">APPLIED-DATE</th>
                                    <th scope="col">PAID</th>
                                    <th scope="col">EDIT</th>
                                    <th scope="col">DELETE</th>
                                  </tr>
                                </thead>
                                <tbody>
                            <% for(testApp of testApps){ %>
                                <% x++ %>
                                              <tr>
                                                <th scope="row"><%=x%></th>
                                                <td class="text-capitalize"><%=testApp.username%></td>
                                                <td><%=testApp.email%></td>
                                                <td class="text-capitalize"><%=testApp.contact%></td>
                                                <td class="text-capitalize"><%=testApp.gender%></td>
                                                <td class="text-capitalize"><%=testApp.dob%></td>
                                                <td class="text-capitalize"><%=testApp.degree%></td>
                                                <td class="text-capitalize"><%=testApp.address%></td>
                                                <td class="text-capitalize"><%=testApp.testCity%></td>
                                                <td class="text-capitalize"><%=testApp.testDate%></td>
                                                <td class="text-capitalize"><%=testApp.testTime%></td>
                                                <td class="text-capitalize"><%=testApp.testAppliedDate%></td>
                                                <td class="text-capitalize"><%=testApp.paid%></td>
                                                <td class="text-capitalize">
                                                  <a href="/admin/edit-test-application/<%=testApp._id%>?edit=true" class="btn btn-primary bg-gradient-primary btn-sm m-0">Edit</a>
                                                </td>
                                                
                                                <td>
                                                    <form action="/admin/delete-test-application" method="POST" class="m-0 p-0">
                                                    <input type="hidden" name="_csrf" value="<%=csrfToken%>">
                                                    <input type="hidden" name="testAppId" value="<%=testApp._id%>">
                                                    <button class="btn btn-danger bg-gradient-danger btn-sm">Delete</button>
                                                    </form>
                                                </td>
                                              </tr>
                                <% } %>
                            </tbody>
                        </table>
                  </div>
                    <% } else { %>
                        <h3 class="text-default"> No Test Applicants Found! :(</h3>
                        <% } %>
               <% } else if(path === '/admin/recruiter-apps') { %>
                <h2 class="text-center mt-3 mb-5">Registered Recruiter Applicants </h2>
                <% if(recapps.length>0) { %>
                    <% var x = 0; %>
                  <div class="table-responsive">
                        <table class="table table-hover table-striped table-bordered table-dark">
                                <caption>List of Recruiter Applicants</caption>
                                <thead>
                                  <tr>
                                    <th scope="col">#S.NO</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Phone</th>
                                    <th scope="col">Designation</th>
                                    <th scope="col">Company</th>
                                    <th scope="col">Profile</th>
                                    <th scope="col">Description</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Delete</th>
                                  </tr>
                                </thead>
                                <tbody>
                            <% for(recapp of recapps){ %>
                                <% x++ %>
                                              <tr>
                                                <th scope="row"><%=x%></th>
                                                <td class="text-capitalize"><%=recapp.name%></td>
                                                <td><%=recapp.email%></td>
                                                <td class="text-capitalize"><%=recapp.phone%></td>
                                                <td class="text-capitalize"><%=recapp.designation%></td>
                                                <td class="text-capitalize"><%=recapp.company%></td>
                                                <td class="text-capitalize"><%=recapp.profile%></td>
                                                <td class="text-capitalize"><%=recapp.description%></td>
                                                <td class="text-capitalize"><%=recapp.createdAt%></td>
                                                <td>
                                                    <form action="/admin/delete-recapp" method="POST" class="m-0 p-0">
                                                    <input type="hidden" name="_csrf" value="<%=csrfToken%>">
                                                    <input type="hidden" name="recId" value="<%=recapp._id%>">
                                                    <button class="btn btn-danger bg-gradient-danger btn-sm">Delete</button>
                                                    </form>
                                                </td>
                                              </tr>
                                <% } %>
                            </tbody>
                        </table>
                  </div>
                    <% } else { %>
                      <h3 class="text-default"> No Recruiter Applicants Found! :(</h3>
                      <% } %>       
               <% } else if(path === '/admin/test-payments') { %> 
