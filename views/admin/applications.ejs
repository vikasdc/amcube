                <% if(testpayments.length>0) { %>
                    <% var x = 0; %>
                  <div class="table-responsive">
                        <table class="table table-hover table-striped table-bordered table-dark">
                                <caption>List of Test Payments </caption>
                                <thead>
                                  <tr>
                                    <th scope="col">#S.NO</th>
                                    <th scope="col">PAYMENT_ID</th>
                                    <th scope="col">STATUS</th>
                                    <th scope="col">AMOUNT</th>
                                    <th scope="col">NAME</th>
                                    <th scope="col">EMAIL</th>
                                    <th scope="col">PHONE</th>
                                    <th scope="col">TEST DATE</th>
                                    <th scope="col">TEST TIME</th>
                                    <th scope="col">TEST CITY</th>
                                    <th scope="col">Created At</th>
                                    <th scope="col">DELETE</th>
                                  </tr>
                                </thead>
                                <tbody>
                            <% for(testpayment of testpayments){ %>
                                <% x++ %>
                                              <tr>
                                                <th scope="row"><%=x%></th>
                                                <td class="text-capitalize"><%=testpayment.payment_id%></td>
                                                <td class="text-capitalize"><%=testpayment.status%></td>
                                                <td class="text-capitalize"><%=testpayment.amount%></td>
                                                <td class="text-capitalize"><%=testpayment.buyer_name%></td>
                                                <td class=""><%=testpayment.buyer_email%></td>
                                                <td class="text-capitalize"><%=testpayment.buyer_phone%></td>
                                                <td class="text-capitalize"><%=testpayment.testDate%></td>
                                                <td class="text-capitalize"><%=testpayment.testTime%></td>
                                                <td class="text-capitalize"><%=testpayment.testCity%></td>
                                                <td class="text-capitalize"><%=testpayment.createdAt%></td>
                                                
                                                <td>
                                                    <form action="/admin/delete-test-payment" method="POST" class="m-0 p-0">
                                                    <input type="hidden" name="_csrf" value="<%=csrfToken%>">
                                                    <input type="hidden" name="testpaymentId" value="<%=testpayment._id%>">
                                                    <button class="btn btn-danger bg-gradient-danger btn-sm">Delete</button>
                                                    </form>
                                                </td>
                                              </tr>
                                <% } %>
                            </tbody>
                        </table>
                  </div>
                    </div>
                    <% } else { %>
                        <h3 class="text-default"> No Test Payments Found! :(</h3>
                        <% } %>
                <% } else if(path === '/admin/manage-courseapplications') { %>
                <h2 class="text-center mt-3 mb-5">Registered Course Applicants</h2>
                <% if(courseApps.length>0) { %>
                    <% var x = 0; %>
                  <div class="table-responsive">
                        <table class="table table-hover table-striped table-bordered table-dark">
                                <caption>List of Course Applicants </caption>
                                <thead>
                                  <tr>
                                    <th scope="col">#S.NO</th>
                                    <th scope="col">USERNAME</th>
                                    <th scope="col">EMAIL</th>
                                    <th scope="col">CONTACT</th>
                                    <th scope="col">COURSE-NAME</th>
                                    <th scope="col">PRICE</th>
                                    <th scope="col">ENROLL-DATE</th>
                                    <th scope="col">PAID</th>
                                    <th scope="col">EDIT</th>
                                    <th scope="col">DELETE</th>
                                  </tr>
                                </thead>
                                <tbody>
                            <% for(courseApp of courseApps){ %>
                                <% x++ %>
                                              <tr>
                                                <th scope="row"><%=x%></th>
                                                <td class="text-capitalize"><%=courseApp.username%></td>
                                                <td><%=courseApp.email%></td>
                                                <td class="text-capitalize"><%=courseApp.contact%></td>
                                                <td class="text-capitalize"><%=courseApp.courseName%></td>
                                                <td class="text-capitalize"><%=courseApp.coursePrice%></td>
                                                <td class="text-capitalize"><%=courseApp.courseEnrollDate%></td>
                                                <td class="text-capitalize"><%=courseApp.paid%></td>
                                                <td>
                                                  <a href="/admin/edit-course-application/<%=courseApp._id%>?edit=true" class="m-0 btn btn-primary bg-gradient-primary btn-sm">Edit
                                                  </a>
                                              </td>
                                                <td>
                                                    <form action="/admin/delete-course-application" method="POST" class="m-0 p-0">
                                                    <input type="hidden" name="_csrf" value="<%=csrfToken%>">
                                                    <input type="hidden" name="courseAppId" value="<%=courseApp._id%>">
                                                    <button class="btn btn-danger bg-gradient-danger btn-sm">Delete</button>
                                                    </form>
                                                </td>
                                              </tr>
                                <% } %>
                            </tbody>
                        </table>
                  </div>
                    </div>
                    <% } else { %>
                        <h3 class="text-default"> No Course Applicants Found! :(</h3>
                        <% } %>
                    <% } else if(path === '/admin/manage-jobapplications') { %>
                        <h2 class="text-center mt-3 mb-5">Registered Job Applicants </h2>
                        <% if(jobApps.length>0) { %>
